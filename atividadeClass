class Carro():
    def __init__(self, km, combustivel):
        self.combustivel = combustivel
        self.km = km

carro1 = Carro(km=200, combustivel=10)
carro2 = Carro(km=300, combustivel=10)
print(carro1.km / carro1.combustivel)

